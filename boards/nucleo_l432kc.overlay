/ {
	aliases {
		gnss = &gnss;
        gps-usart = &usart1;
	};
};

&usart1 { 
    pinctrl-0 = <&usart1_tx_pa9 &usart1_rx_pa10>; 
    pinctrl-names = "default"; 
    current-speed = <9600>; 
    status = "okay";
    gnss: gnss {
        compatible = "gnss-nmea-generic";
        status = "okay";

    };
};